 <script type="text/javascript">
	
	    
      
      var isPopup = $$('body.popup').size() > 0;
      var courseId = null;
      var groupId = null;
      var showAll = isPopup || courseId!=null || groupId!=null ;
      
      notification_controller.setLimitNotificationLoad( !showAll );
	    ews_model.initEws(courseId);
      
	    var alertPastDueViewObj = new AlertsView( courseId, groupId,"alertsModule", "alertPastDueView", {"TE": ["OVERDUE"],"SU": ["OVERDUE"],"GB": ["OVERDUE"],"AS": ["OVERDUE"]},
	    		                                      {"AS::OVERDUE": ["AS::OVERDUE"],"GB::OVERDUE": ["GB::OVERDUE"],"TE::OVERDUE": ["TE::OVERDUE"],"SU::OVERDUE": ["SU::OVERDUE"]}, undefined, showAll );
	    var alertEwsViewObj = new EwsView(courseId, groupId,"alertsModule", "alertEwsView", undefined, showAll );
	    var alertActivityViewObj = new ActivityAlertsView( courseId, groupId, "alertsModule", "alertActivityView",
                                                         {"QU": ["QU_AVAIL"]}, {"QU::QU_AVAIL": ["QU::QU_AVAIL"]},
                                                         showAll );
	    
      
	    
	    module_controller.addModule("alertsModule", [alertPastDueViewObj.viewId]);
      
	    alertPastDueViewObj.display();
	    alertEwsViewObj.display();
	    alertActivityViewObj.display();
	    
	    alertPastDueViewObj.createViewMenu("alertsPastDueViewMenu");
	    
	    </script>
	  
 
