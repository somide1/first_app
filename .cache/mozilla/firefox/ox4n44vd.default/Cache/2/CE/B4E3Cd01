<script type="text/javascript">

var inlineReceiptMsgs = [];

// store receipt message ids while rendering the page and then update them so screen reader spews them out as alerts
  
function addMsg( id )
{
  inlineReceiptMsgs.push( { id : id } );
}

FastInit.addOnLoad( function(){
 for ( var i = 0; i < inlineReceiptMsgs.length; i++ )
  {
    (function( idx ){ 
      var receipt = $( inlineReceiptMsgs[idx].id );
      receipt.setAttribute("aria-live", "assertive");
      receipt.setAttribute("aria-atomic", "true");
      if( idx === 0)
        receipt.setAttribute("role", "alert");
       else
        receipt.setAttribute("role", "status"); //prevent JAWS disrupt other receipt msgs
      receipt.update( receipt.innerHTML );
    }).delay( i*2 + 1, i );  // stagger receipts so screen reader reads them sequentially
  }   
 } );

</script>

